# EasyPost API Configuration
EASYPOST_API_KEY=your_easypost_api_key_here

# Veeqo API Configuration
VEEQO_TOKEN=your_veeqo_token_here

# Origin Address Information
ORIGIN_NAME=Your Company Name
ORIGIN_STREET1=123 Main Street
ORIGIN_STREET2=Suite 100
ORIGIN_CITY=San Francisco
ORIGIN_STATE=CA
ORIGIN_ZIP=94105
ORIGIN_COUNTRY=US
ORIGIN_PHONE=555-123-4567

# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration (SQLite)
# Set to false to disable database persistence
ENABLE_DATABASE=true
DATABASE_PATH=./data/orders.db

# Authentication Configuration
# Set to true to enable Google OAuth (requires additional setup)
ENABLE_GOOGLE_AUTH=false

# Google OAuth Configuration (Required only if ENABLE_GOOGLE_AUTH=true)
# To enable:
# 1. Create OAuth credentials at https://console.cloud.google.com/apis/credentials
# 2. Set authorized redirect URI to: https://your-domain.com/auth/google/callback
# 3. Uncomment and fill in the values below
# GOOGLE_CLIENT_ID=your_google_client_id_here
# GOOGLE_CLIENT_SECRET=your_google_client_secret_here
# GOOGLE_REDIRECT_URI=https://your-domain.com/auth/google/callback
# SESSION_SECRET=your_random_session_secret_here
